.main{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.photo-getter{
  position: relative;
  display: flex;
}
.photo-getter,.camera-border,.camera,.photo{
  width: 500rpx;
  height: 500rpx;
}
.camera-border{
  z-index: 2;
  background-color: transparent;
  position: absolute;
  left: 0px;
  right: 0px;
  top: 0px;
}
.tip{
  color: #606266;
  text-align: center;
}
.result{
  text-align: center;
  font-weight: 800;
}
.rank button{
  margin-bottom: 50rpx;
}