.tabbar{
  height: 120rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  background-color: rgba(255,255,255,0.5);
}
.tab{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tip{
  border: 2rpx solid black;
  border-radius: 50rpx;
  width: 100rpx;
  height: 100rpx;
  overflow: hidden;
  font-size: 20rpx;
}
.icon{
  height: 60rpx;
  width: 60rpx;
}
